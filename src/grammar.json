{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "asciidoc",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "block"
      }
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section"
        },
        {
          "type": "SYMBOL",
          "name": "attribute_entry"
        },
        {
          "type": "SYMBOL",
          "name": "blank_line"
        },
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "blank_line": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[ \\t]"
            }
          },
          {
            "type": "SYMBOL",
            "name": "newline"
          }
        ]
      }
    },
    "_attr_name": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_][A-Za-z0-9_-]*"
    },
    "_attr_value": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    },
    "attribute_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "_attr_name"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_attr_value"
          }
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^=:\\n][^\\n]*"
        }
      }
    },
    "paragraph": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT1",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "text"
                },
                {
                  "type": "SYMBOL",
                  "name": "newline"
                }
              ]
            }
          }
        ]
      }
    },
    "title": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "section_title": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": "="
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": "=="
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": "==="
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": "===="
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": "====="
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": "======"
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        }
      ]
    },
    "section": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "title",
            "content": {
              "type": "SYMBOL",
              "name": "section_title"
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "blank_line"
                },
                {
                  "type": "SYMBOL",
                  "name": "attribute_entry"
                },
                {
                  "type": "SYMBOL",
                  "name": "paragraph"
                },
                {
                  "type": "SYMBOL",
                  "name": "section"
                }
              ]
            }
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [
    "block"
  ],
  "reserved": {}
}