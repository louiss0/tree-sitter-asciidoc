{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "asciidoc",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_block"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "attribute_entry"
        },
        {
          "type": "SYMBOL",
          "name": "section"
        },
        {
          "type": "PREC",
          "value": 15,
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "fake_heading_paragraph"
            },
            "named": true,
            "value": "paragraph"
          }
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "invalid_attribute_paragraph"
            },
            "named": true,
            "value": "paragraph"
          }
        },
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "invalid_attribute_paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PATTERN",
                    "value": ": [^\\r\\n]*"
                  }
                },
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PREC",
                    "value": 12,
                    "content": {
                      "type": "PATTERN",
                      "value": ":incomplete because no colon"
                    }
                  }
                },
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PREC",
                    "value": 12,
                    "content": {
                      "type": "PATTERN",
                      "value": ":incomplete-because-no-second-colon"
                    }
                  }
                },
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PATTERN",
                    "value": "::[^\\r\\n]*"
                  }
                },
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PATTERN",
                    "value": ":::[^\\r\\n]*"
                  }
                }
              ]
            },
            "named": true,
            "value": "text"
          }
        }
      ]
    },
    "fake_heading_paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PATTERN",
                    "value": "={1,6}[^ \\t\\r\\n][^\\r\\n]*"
                  }
                },
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PREC",
                    "value": 20,
                    "content": {
                      "type": "PATTERN",
                      "value": "====== Also not a heading"
                    }
                  }
                },
                {
                  "type": "TOKEN",
                  "content": {
                    "type": "PATTERN",
                    "value": "[ \\t]+={1,6}[^\\r\\n]*"
                  }
                }
              ]
            },
            "named": true,
            "value": "text"
          }
        }
      ]
    },
    "section": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "section_title"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "attribute_entry"
                },
                {
                  "type": "SYMBOL",
                  "name": "paragraph"
                },
                {
                  "type": "PREC",
                  "value": -1,
                  "content": {
                    "type": "SYMBOL",
                    "name": "section"
                  }
                }
              ]
            }
          }
        ]
      }
    },
    "section_title": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 10,
            "content": {
              "type": "PATTERN",
              "value": "={1,6}[ \\t]+"
            }
          }
        },
        {
          "type": "FIELD",
          "name": "title",
          "content": {
            "type": "SYMBOL",
            "name": "title"
          }
        }
      ]
    },
    "title": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n]+"
      }
    },
    "paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        }
      ]
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\r\\n][^\\r\\n]*(?:\\r?\\n[^\\r\\n:=][^\\r\\n]*)*"
        }
      }
    },
    "attribute_entry": {
      "type": "PREC",
      "value": 20,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "STRING",
              "value": ":"
            }
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "ALIAS",
              "content": {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PATTERN",
                  "value": "[A-Za-z0-9][A-Za-z0-9_-]*"
                }
              },
              "named": true,
              "value": "name"
            }
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": ":"
            }
          },
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "value"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          }
        ]
      }
    },
    "name": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z0-9][A-Za-z0-9_-]*"
      }
    },
    "value": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n]+"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}