{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "asciidoc",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "section"
          },
          {
            "type": "SYMBOL",
            "name": "attribute_entry"
          },
          {
            "type": "SYMBOL",
            "name": "paragraph"
          }
        ]
      }
    },
    "_section_marker": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 10,
        "content": {
          "type": "PATTERN",
          "value": "={1,6}[ \\t]+"
        }
      }
    },
    "title": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n]+"
      }
    },
    "section_title": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_section_marker"
        },
        {
          "type": "SYMBOL",
          "name": "title"
        }
      ]
    },
    "section": {
      "type": "SYMBOL",
      "name": "section_title"
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\r\\n][^\\r\\n]*"
        }
      }
    },
    "paragraph": {
      "type": "SYMBOL",
      "name": "text"
    },
    "name": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_-]*"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "attribute_entry": {
      "type": "PREC_DYNAMIC",
      "value": 5,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": ":"
          },
          {
            "type": "SYMBOL",
            "name": "name"
          },
          {
            "type": "STRING",
            "value": ":"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}