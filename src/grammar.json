{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "asciidoc",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "block"
      }
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section"
        },
        {
          "type": "SYMBOL",
          "name": "attribute_entry"
        },
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_][A-Za-z0-9_-]*"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\r\\n]*"
    },
    "attribute_entry": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "STRING",
              "value": ":"
            }
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "name"
            }
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "STRING",
              "value": ":"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[ \\t]+"
                  },
                  {
                    "type": "FIELD",
                    "name": "value",
                    "content": {
                      "type": "SYMBOL",
                      "name": "value"
                    }
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^=\\r\\n][^\\r\\n]*"
        }
      }
    },
    "paragraph": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "text"
        }
      }
    },
    "title": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "section_title": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 2,
                "content": {
                  "type": "STRING",
                  "value": "="
                }
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 2,
                "content": {
                  "type": "STRING",
                  "value": "=="
                }
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 2,
                "content": {
                  "type": "STRING",
                  "value": "==="
                }
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 2,
                "content": {
                  "type": "STRING",
                  "value": "===="
                }
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 2,
                "content": {
                  "type": "STRING",
                  "value": "====="
                }
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 2,
                "content": {
                  "type": "STRING",
                  "value": "======"
                }
              }
            },
            {
              "type": "PATTERN",
              "value": "[ \\t]+"
            },
            {
              "type": "FIELD",
              "name": "title",
              "content": {
                "type": "SYMBOL",
                "name": "title"
              }
            }
          ]
        }
      ]
    },
    "section": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "title",
            "content": {
              "type": "SYMBOL",
              "name": "section_title"
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "attribute_entry"
                },
                {
                  "type": "SYMBOL",
                  "name": "paragraph"
                },
                {
                  "type": "SYMBOL",
                  "name": "section"
                }
              ]
            }
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [
    "block"
  ],
  "reserved": {}
}