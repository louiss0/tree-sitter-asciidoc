{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "asciidoc",
      "camelcase": "AsciiDoc",
      "title": "AsciiDoc",
      "scope": "source.asciidoc",
      "file-types": [
        "adoc",
        "asciidoc"
      ],
      "injection-regex": "^asciidoc$",
      "class-name": "TreeSitterAsciidoc",
      "highlights": "queries/highlights.scm",
      "indents": "queries/indents.scm",
      "injections": "queries/injections.scm",
      "folds": "queries/folds.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for AsciiDoc",
    "authors": [
      {
        "name": "Shelton Louis",
        "email": "louisshelton0@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/louiss0/tree-sitter-asciidoc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
